
public class ThreadMain {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Thread1 t1 = new Thread1();
		try{
		for(int i=0; i<10; i++)
		{
			System.out.println("Thread Main " + i);
			Thread.sleep(1000);
		}
		}
		catch(InterruptedException e)
		{
			
		}
		
		
	}

}
